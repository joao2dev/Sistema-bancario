public class banco {
    private final int numeroDaConta;
    private String NomeDoTitular;
    private double saldo;

    public banco(int numero, String titular) {
        this.numeroDaConta = numero;
        this.NomeDoTitular = titular;
        this.saldo = 0.0;
    }

    public banco(int numero, String titular, double saldoInicial) {
        this.numeroDaConta = numero;
        this.NomeDoTitular = titular;
        this.saldo = saldoInicial;
    }

    public void depositar(double valor) {
        saldo += valor;
    }



    public String sacar(double valor) {
        double taxa = 5.75;
        saldo -= valor + taxa;
        return "Saque realizado com sucesso. Saldo atual: R$" + saldo;

    }

    public double getSaldo() {
        return saldo;
    }

    public String getNomeDoTitular() {
        return NomeDoTitular;
    }

    public int getNumeroDaConta() {
        return numeroDaConta;
    }

}
